    <LineSymbolizer stroke="#000000" stroke-opacity="1" />
    <PolygonSymbolizer fill-opacity="1" />
  </Rule>
</Style>
<Layer name="vegetation"
  srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0.0 +k=1.0 +units=m +nadgrids=@null +wktext +no_defs +over">
    <StyleName>vegetation</StyleName>
    <StyleName>vegetation-class</StyleName>
    <StyleName>vegetation-macrogroup</StyleName>
    <StyleName>vegetation-alliance</StyleName>  </Layer>

</Map>